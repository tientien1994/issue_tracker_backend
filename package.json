    "db:index": "node_modules/.bin/babel-node ./src/migrations/indexing/index.js",
    "db:index:reset": "node_modules/.bin/babel-node ./src/migrations/indexing/reset.js",
    "db:seed": "node_modules/.bin/babel-node ./src/seeder/index.js",
    "import:company": "node_modules/.bin/babel-node ./src/imports/importCompanies.js"
  },
  "dependencies": {
    "@googlemaps/google-maps-services-js": "^3.2.6",
    "@graphql-tools/schema": "^8.1.2",
    "agenda": "^3.1.0",
    "agendash": "^0.4.0",
    "apollo-link-ws": "^1.0.19",
    "apollo-server-core": "^3.4.0",
    "apollo-server-express": "^3.1.2",
    "aws-sdk": "^2.1023.0",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-polyfill": "^6.26.0",
    "base-64": "^1.0.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "cron-parser": "^4.1.0",
    "dotenv": "^10.0.0",
    "ejs": "^3.1.6",
    "email-templates": "^8.0.8",
    "exceljs": "^4.3.0",
    "express": "^4.17.1",
    "express-basic-auth": "^1.2.0",
    "get-graphql-schema": "^2.1.1",
    "graphql": "^14.5.8",
    "graphql-redis-subscriptions": "^1.3.1",
    "graphql-subscriptions": "^0.5.4",
    "graphql-tools": "^6.0.7",
    "graphql-type-json": "^0.3.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "mongodb": "^4.1.3",
    "node-fetch": "^2.3.0",
    "nodemailer": "^6.7.1",
    "pluralize": "^8.0.0",
    "redis": "^3.1.2",
    "request": "^2.88.2",
    "subscriptions-transport-ws": "^0.9.19",
    "validate.js": "^0.13.1"
