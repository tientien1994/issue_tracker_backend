{
  "name": "glife-map-service",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "serve": "node dist/index.js",
    "start": "node_modules/.bin/nodemon --exec babel-node src/index.js",
